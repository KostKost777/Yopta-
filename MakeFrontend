FLAGS = -g -DDEBUG
all: front

front: orig_lexicon_of_lang.o hash_funcs.o read_from_file_funcs.o frontend_main.o lexical_analysis.o
	g++ $(FLAGS) orig_lexicon_of_lang.o hash_funcs.o read_from_file_funcs.o frontend_main.o lexical_analysis.o -o front

frontend_main.o: frontend_main.cpp
	g++ $(FLAGS) -c frontend_main.cpp

lexical_analysis.o: lexical_analysis.cpp
	g++ $(FLAGS) -c lexical_analysis.cpp

read_from_file_funcs.o: read_from_file_funcs.cpp
	g++ $(FLAGS) -c read_from_file_funcs.cpp

hash_funcs.o: hash_funcs.cpp
	g++ $(FLAGS) -c hash_funcs.cpp

orig_lexicon_of_lang.o: orig_lexicon_of_lang.cpp
	g++ $(FLAGS) -c orig_lexicon_of_lang.cpp
